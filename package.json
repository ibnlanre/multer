{
  "name": "@ibnlanre/multer",
  "version": "0.0.0",
  "description": "Multer storage mocks",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.cjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "module",
  "scripts": {
    "test": "vitest --run",
    "posttest": "vitest run --coverage",
    "preversion": "npm test",
    "version": "git add .",
    "postversion": "npm publish --access public",
    "prepublishOnly": "tsup",
    "postpack": "git push",
    "postpublish": "git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibnlanre/multer.git"
  },
  "keywords": [
    "multer",
    "mock",
    "express",
    "stub"
  ],
  "author": "Ridwan Olanrewaju",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ibnlanre/multer/issues"
  },
  "homepage": "https://github.com/ibnlanre/multer#readme",
  "devDependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@types/express": "^5.0.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^2.1.9",
    "aws-sdk": "^2.1692.0",
    "express": "^5.1.0",
    "multer": "^1.4.5-lts.2",
    "multer-cloud-storage": "^4.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "vite": "^5.4.18",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.9"
  }
}
